<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<appedner name="STDOUT"
		class="ch.pos.logback.core.ConsoleAppender">
		<layout class="ch.pos.logback.classic.PatternLayout">
			<pattern>%d{HH:mm:ss.SSS}[%-5level] - %m%n</pattern>
		</layout>

	</appedner>
	<root level="DEBUG">
		<appender-ref ref="STDOUT" />
	</root>
</configuration>